{{#each this}}
    {{#if this.name}}
        <div id = "randombeer-card">
           {{>beerpartial }}
            <h2>Food Pairing</h2>
            <ul id="pairing">
                {{#each food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <h2 id="tips">Brewers Tips: <span style="font-weight: lighter">{{brewers_tips}}</span></h2>
        </div>
    {{/if}}
{{/each}} 